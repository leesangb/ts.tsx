{
  "name": "tstsx",
  "version": "0.0.4",
  "description": "Type-safe utilities for React and TypeScript applications",
  "license": "MIT",
  "type": "module",
  "author": "Sangbin Lee <leesangbin@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/leesangb/ts.tsx.git"
  },
  "homepage": "https://github.com/leesangb/ts.tsx#readme",
  "bugs": {
    "url": "https://github.com/leesangb/ts.tsx/issues"
  },
  "keywords": [
    "react",
    "typescript",
    "utilities",
    "type-safe",
    "suspense",
    "error-boundary",
    "navigation",
    "hooks"
  ],
  "exports": {
    "./combined": {
      "types": "./dist/combined.d.ts",
      "import": "./dist/combined.js"
    },
    "./exception-boundary": {
      "types": "./dist/exception-boundary.d.ts",
      "import": "./dist/exception-boundary.js"
    },
    "./init": {
      "types": "./dist/init.d.ts",
      "import": "./dist/init.js"
    },
    "./object-diff": {
      "types": "./dist/object-diff.d.ts",
      "import": "./dist/object-diff.js"
    },
    "./preventable": {
      "types": "./dist/preventable.d.ts",
      "import": "./dist/preventable.js"
    },
    "./stack-navigation": {
      "types": "./dist/stack-navigation.d.ts",
      "import": "./dist/stack-navigation.js"
    },
    "./suspensify": {
      "types": "./dist/suspensify.d.ts",
      "import": "./dist/suspensify.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "node ../../scripts/generate-exports.mjs",
    "build": "vite build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tstsx/combined": "workspace:*",
    "@tstsx/exception-boundary": "workspace:*",
    "@tstsx/init": "workspace:*",
    "@tstsx/object-diff": "workspace:*",
    "@tstsx/preventable": "workspace:*",
    "@tstsx/stack-navigation": "workspace:*",
    "@tstsx/suspensify": "workspace:*",
    "@types/react": "catalog:react18",
    "react": "catalog:react18",
    "typescript": "catalog:core",
    "vite": "catalog:tools",
    "vite-plugin-dts": "catalog:tools"
  },
  "publishConfig": {
    "access": "public"
  }
}
