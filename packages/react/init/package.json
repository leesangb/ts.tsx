{
  "name": "@tstsx/init",
  "version": "0.0.4",
  "description": "React component initializer with async data loading support",
  "license": "MIT",
  "type": "module",
  "author": "Sangbin Lee <leesangbin@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/leesangb/ts.tsx.git"
  },
  "keywords": [
    "react",
    "typescript",
    "initializer",
    "async",
    "data-loading",
    "hoc",
    "higher-order-component"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rf .turbo dist",
    "dev": "vitest",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0",
    "react-dom": "^18.0 || ^19.0"
  },
  "dependencies": {
    "@tstsx/suspensify": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/dom": "catalog:test",
    "@testing-library/jest-dom": "catalog:test",
    "@testing-library/react": "catalog:test",
    "@testing-library/user-event": "catalog:test",
    "@types/react": "catalog:react18",
    "@types/react-dom": "catalog:react18",
    "@vitejs/plugin-react": "catalog:tools",
    "jsdom": "catalog:test",
    "typescript": "catalog:core",
    "vite": "catalog:tools",
    "vite-plugin-dts": "catalog:tools",
    "vitest": "catalog:test"
  },
  "publishConfig": {
    "access": "public"
  }
}
